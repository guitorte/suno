  {
    "id": "mapping-8",
    "headers": [
      "Standard Spelling",
      "Phonetic Respelling",
      "Why"
    ],
    "entries": [
      {
        "standard-spelling": "**read** (present tense)",
        "phonetic-respelling": "`reed`",
        "why": "Forces \"ree-d\" instead of \"red\""
      },
      {
        "standard-spelling": "**live** (as in concert)",
        "phonetic-respelling": "`lyve`",
        "why": "Forces \"laiv\" instead of \"liv\""
      },
      {
        "standard-spelling": "**bass** (instrument/low frequency)",
        "phonetic-respelling": "`bahss` or `basss`",
        "why": "Avoids \"base\" pronunciation"
      },
      {
        "standard-spelling": "**tear** (crying)",
        "phonetic-respelling": "`teer`",
        "why": "Forces \"teer\" instead of \"tare\""
      },
      {
        "standard-spelling": "**wound** (injury)",
        "phonetic-respelling": "`woond`",
        "why": "Forces \"woond\" instead of \"wownd\""
      },
      {
        "standard-spelling": "**lead** (metal)",
        "phonetic-respelling": "`led`",
        "why": "Forces \"led\" instead of \"leed\""
      }
    ]
  }
